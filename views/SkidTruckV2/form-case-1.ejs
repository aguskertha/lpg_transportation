<div class="row">
    <div class="col-md-5">
        <h2>Skid Truck ID</h2>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="mb-3">
            <label class="form-label">Cargo Capacity</label>
            <div class="input-group mb-3">
            <input class="form-control" list="datalistOptions" id="skidTruckCargoCapacity" name="skidTruckCargoCapacity" placeholder="Select Cargo Capacity" value="<%= typeof skidTruckCargoCapacity != 'undefined' ? skidTruckCargoCapacity : '' %>" required>
            <datalist id="datalistOptions">
                <option value="" selected>Choose Doctor</option>
                <option value="5">5</option>
                <option value="8">8</option>
                <option value="11">11</option>
                <option value="13">13</option>
                <option value="15">15</option>
                <option value="18">18</option>
                <option value="20">20</option>
            </datalist>
            <div class="input-group-append">
                <span class="input-group-text">MT</span>
            </div>
            </div>
        </div>
        <div class="mb-3">
            <label class="form-label">Head Truck Specification</label>
            <div class="input-group mb-3">
            <input type="text" class="form-control" id="skidTruckHeadSpec" name="skidTruckHeadSpec" value="<%= typeof skidTruckHeadSpec != 'undefined' ? skidTruckHeadSpec : '' %>">
            <div class="input-group-append">
                <span class="input-group-text">PS</span>
