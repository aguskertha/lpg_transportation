<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>
    $(function(){
        var unitConversionStr = $('#unitConversion').val();
        var unitConversion = JSON.parse(unitConversionStr);

        $('#skidTruckCargoCapacity').on('change', function () {
            var skidTruckCargoCapacity = $('#skidTruckCargoCapacity').val();
            var skidTruckFuelConsume = 0
            var basisDataRentalPriceMonth = 0
            if(skidTruckCargoCapacity == 5){
                skidTruckFuelConsume = 4.6
                basisDataRentalPriceMonth = 19500000
            }
            else if (skidTruckCargoCapacity == 8){
                skidTruckFuelConsume = 4.8
                basisDataRentalPriceMonth = 30000000
            }
            else if (skidTruckCargoCapacity == 11){
                skidTruckFuelConsume = 4.9
                basisDataRentalPriceMonth = 42000000
            }
            else if (skidTruckCargoCapacity == 13){
                skidTruckFuelConsume = 5
                basisDataRentalPriceMonth = 46000000
            }
            else if (skidTruckCargoCapacity == 15){
                skidTruckFuelConsume = 5
                basisDataRentalPriceMonth = 50000000
            }
            else if (skidTruckCargoCapacity == 18){
                skidTruckFuelConsume = 5.1
                basisDataRentalPriceMonth = 60000000
            }
            else if (skidTruckCargoCapacity == 20){
                skidTruckFuelConsume = 5.4
                basisDataRentalPriceMonth = 66300000
            }
            $('#skidTruckFuelConsume').val(skidTruckFuelConsume)
            $('#basisDataRentalPriceMonth').val(basisDataRentalPriceMonth)
            $('#basisDataRentalPriceDay').val(basisDataRentalPriceMonth / 30)
        });
        $('#bunkerPriceVesselRp').on('change', function () {
            var bunkerPriceVesselRp = $('#bunkerPriceVesselRp').val();
            var bunkerPriceVesselUSD = Number(bunkerPriceVesselRp) * 1000 / unitConversion.USDtoRP
            var bunkeringBunkerPriceUSD = Number(bunkerPriceVesselUSD)
            
            $('#bunkerPriceVesselUSD').val(bunkerPriceVesselUSD)
            $('#bunkeringBunkerPriceUSD').val(bunkeringBunkerPriceUSD)
            $('#bunkeringBunkerPriceIDR').val(bunkeringBunkerPriceUSD * unitConversion.USDtoRP)
        });


        $('#effectiveDateDelivery').on('change', function () {
            var effectiveDateDelivery = $('#effectiveDateDelivery').val();
            var dependentDocking = $('#dependentDocking').val();
            var effectiveDeliveryDay = Number(effectiveDateDelivery) - Number(dependentDocking)
            $('#effectiveDeliveryDay').val(effectiveDeliveryDay)
        });
        $('#dependentDocking').on('change', function () {
            var effectiveDateDelivery = $('#effectiveDateDelivery').val();
            var dependentDocking = $('#dependentDocking').val();
            var effectiveDeliveryDay = Number(effectiveDateDelivery) - Number(dependentDocking)
            $('#effectiveDeliveryDay').val(effectiveDeliveryDay)
        });
        $('#bargeCargoCapacityCBM').on('change', function () {
            var bargeCargoCapacityCBM = $('#bargeCargoCapacityCBM').val();
            var bargeCargoCapacityMT = Number(bargeCargoCapacityCBM * unitConversion.CBMtoMT)
            $('#bargeCargoCapacityMT').val(bargeCargoCapacityMT)
        });
        $('#bargeCargoCapacityMT').on('change', function () {
            var bargeCargoCapacityMT = $('#bargeCargoCapacityMT').val();
            var bargeCargoCapacityCBM = Number(bargeCargoCapacityMT / unitConversion.CBMtoMT)
            $('#bargeCargoCapacityCBM').val(bargeCargoCapacityCBM)
        });
        $('#bargeYearBuilt').on('change', function () {
            var bargeYearBuilt = $('#bargeYearBuilt').val();
            var currentYear = Number(new Date().getFullYear());
            var bargeShipAge = Number(currentYear - bargeYearBuilt)
            $('#bargeShipAge').val(bargeShipAge)
        });
        $('#tugboatYearBuilt').on('change', function () {
            var tugboatYearBuilt = $('#tugboatYearBuilt').val();
            var currentYear = Number(new Date().getFullYear());
            var tugboatShipAge = Number(currentYear - tugboatYearBuilt)
            $('#tugboatShipAge').val(tugboatShipAge)
        });

        $('#basisDataLPGPriceUSD').on('change', function () {
            var basisDataLPGPriceUSD = $('#basisDataLPGPriceUSD').val();
            var basisDataLPGPriceRp = Number(basisDataLPGPriceUSD * unitConversion.USDtoRP)
            $('#basisDataLPGPriceRp').val(basisDataLPGPriceRp)
            $('#basisDataLPGPriceRpKg').val(basisDataLPGPriceRp/1000)
        });
        $('#basisDataLPGPriceRp').on('change', function () {
            var basisDataLPGPriceRp = $('#basisDataLPGPriceRp').val();
            var basisDataLPGPriceUSD = Number(basisDataLPGPriceRp / unitConversion.USDtoRP)
            $('#basisDataLPGPriceUSD').val(basisDataLPGPriceUSD)
            $('#basisDataLPGPriceRpKg').val(basisDataLPGPriceRp/1000)
        });

        $('#basisDataRentalPriceMonth').on('change', function(){
            var basisDataRentalPriceMonth = $('#basisDataRentalPriceMonth').val();
            var basisDataRentalPriceDay = Number(basisDataRentalPriceMonth / 30 * 1.3);
            $('#basisDataRentalPriceDay').val(basisDataRentalPriceDay);
        });

        $('#basisDataDriverSalaryMonth').on('change', function(){
            var basisDataDriverSalaryMonth = $('#basisDataDriverSalaryMonth').val();
            var basisDataDriverSalaryDay = Number(basisDataDriverSalaryMonth / 30);
            $('#basisDataDriverSalaryDay').val(basisDataDriverSalaryDay);
        });
        $('#basisDataAssistDriverSalaryMonth').on('change', function(){
            var basisDataAssistDriverSalaryMonth = $('#basisDataAssistDriverSalaryMonth').val();
            var basisDataAssistDriverSalaryDay = Number(basisDataAssistDriverSalaryMonth / 30);
            $('#basisDataAssistDriverSalaryDay').val(basisDataAssistDriverSalaryDay);
        });


        $('#skidTruckNumberTruck').on('change', function(){
            var skidTruckNumberTruck = $('#skidTruckNumberTruck').val();
            $('#deliveryTruckNumberTruckingDelivery').val(skidTruckNumberTruck);
        });
        $('#deliveryTruckDistanceTruckingPortKM').on('change', function(){
            var deliveryTruckDistanceTruckingPortKM = $('#deliveryTruckDistanceTruckingPortKM').val();
            var skidTruckSpeed = $('#skidTruckSpeed').val();
            var deliveryTruckTruckingTimePort = Number(deliveryTruckDistanceTruckingPortKM) / (Number(skidTruckSpeed) * 0.87)
            $('#deliveryTruckDistanceTruckingPortMile').val(Number(deliveryTruckDistanceTruckingPortKM) * Number(unitConversion.KMtoMILE));
            $('#deliveryTruckTruckingTimePort').val(deliveryTruckTruckingTimePort);
        });
        $('#deliveryTruckDistanceTruckingSPBEKM').on('change', function(){
            var deliveryTruckDistanceTruckingSPBEKM = $('#deliveryTruckDistanceTruckingSPBEKM').val();
            var skidTruckSpeed = $('#skidTruckSpeed').val();
            var deliveryTruckTruckingTimePort = Number(deliveryTruckDistanceTruckingSPBEKM) / (Number(skidTruckSpeed) * 0.87)
            $('#deliveryTruckDistanceTruckingSPBEMile').val(Number(deliveryTruckDistanceTruckingSPBEKM) * Number(unitConversion.KMtoMILE));
            $('#deliveryTruckTruckingTimeSPBE').val(deliveryTruckTruckingTimePort);
        });
        $('#deliveryTruckUnloadingLCT').on('change', function(){
            var deliveryTruckUnloadingLCT = $('#deliveryTruckUnloadingLCT').val();
            var bargeCargoCapacityTruckWithTank = $('#bargeCargoCapacityTruckWithTank').val();
            $('#deliveryTruckTotalLoadingTimeLCT').val(Number(deliveryTruckUnloadingLCT) * Number(bargeCargoCapacityTruckWithTank));
        });
        $('#deliveryTruckLPGLoadingTimeTruk').on('change', function(){
            var deliveryTruckLPGLoadingTimeTruk = $('#deliveryTruckLPGLoadingTimeTruk').val();
            var bargeCargoCapacityTruckWithTank = $('#bargeCargoCapacityTruckWithTank').val();
            $('#deliveryTruckTotalLPGLoadingTimeTruck').val(Number(deliveryTruckLPGLoadingTimeTruk) * Number(bargeCargoCapacityTruckWithTank));
        });
        $('#deliveryTruckTruckLoadingTimeLCT').on('change', function(){
            var deliveryTruckTruckLoadingTimeLCT = $('#deliveryTruckTruckLoadingTimeLCT').val();
            var bargeCargoCapacityTruckWithTank = $('#bargeCargoCapacityTruckWithTank').val();
            var labelTruckUnloadingTimeLCT = `Truck Unloading Time from LCT/Barge at ${deliveryTruckTruckLoadingTimeLCT}`;
            var wrapperTruckUnloadingTimeLCT = $('.wrapperTruckUnloadingTimeLCT');
            $(wrapperTruckUnloadingTimeLCT).empty();
            $(wrapperTruckUnloadingTimeLCT).append(`
            <h5 class="form-label">${labelTruckUnloadingTimeLCT}</h5>
            `);
            $('#deliveryTruckTotalTruckLoadingTimeLCT').val(Number(deliveryTruckTruckLoadingTimeLCT) * Number(bargeCargoCapacityTruckWithTank));
        });

        $('#deliveryTruckOrigin').on('change', function(){
            var deliveryTruckOrigin = $('#deliveryTruckOrigin').val();
            var labelLPGLoadingTime = `LPG Loading Time to Truck at ${deliveryTruckOrigin}`;
            var wrapperLPGLoadingTime = $('.wrapperLPGLoadingTime');
            $(wrapperLPGLoadingTime).empty();
            $(wrapperLPGLoadingTime).append(`
            <h5 class="form-label">${labelLPGLoadingTime}</h5>
            `);
        });
        $('#deliveryTruckPortOrigin').on('change', function(){
            var deliveryTruckPortOrigin = $('#deliveryTruckPortOrigin').val();
            var labelTruckLoadingTime = `Truck Loading Time to LCT/Barge at ${deliveryTruckPortOrigin}`;
            var wrapperTruckLoadingTime = $('.wrapperTruckLoadingTime');
            $(wrapperTruckLoadingTime).empty();
            $(wrapperTruckLoadingTime).append(`
            <h5 class="form-label">${labelTruckLoadingTime}</h5>
            `);
        });
        
        $('#deliveryVoyagePortDischarge').on('change', function(){
