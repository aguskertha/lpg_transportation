    <div class="mb-3">
        <label class="form-label">Others</label>
        <div class="input-group mb-1">
            <input type="number" step="any" class="form-control" id="bunkeringTugboatEngineOthers" name="bunkeringTugboatEngineOthers" value="<%= typeof bunkeringTugboatEngineOthers != 'undefined' ? bunkeringTugboatEngineOthers : '0' %>" required>
            <div class="input-group-append">
                <span class="input-group-text">USD/Month</span>
            </div>
        </div>
    </div>
</div>
<br>
<div class="row">
