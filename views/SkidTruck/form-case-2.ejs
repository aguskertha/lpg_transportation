            <input type="number" step="any" class="form-control" id="portCostLasUnlasPOLTrip" name="portCostLasUnlasPOLTrip" value="<%= typeof portCostLasUnlasPOLTrip != 'undefined' ? portCostLasUnlasPOLTrip : '0' %>" required>
