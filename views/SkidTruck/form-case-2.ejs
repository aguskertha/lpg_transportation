        <label class="form-label">Oiler</label>
        <div class="input-group mb-1">
            <input type="number" step="any" class="form-control" id="bunkeringTugboatEngineOiler" name="bunkeringTugboatEngineOiler" value="<%= typeof bunkeringTugboatEngineOiler != 'undefined' ? bunkeringTugboatEngineOiler : '0' %>" required>
